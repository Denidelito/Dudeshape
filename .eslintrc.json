{  "parserOptions": {    "ecmaVersion": 8,    "sourceType": "module"  },  "env": {    "jquery": true,    "es6": true,    "browser": true,    "node": true  },  "extends": [    "eslint:recommended",    "google"  ],  "rules": {    "indent": ["error", 4],    "linebreak-style": 0,    "global-require": 0,    "eslint linebreak-style": [0, "error", "windows"],    "no-console": [      "error",      {        "allow": [          "warn",          "error",          "log"        ]      }    ]  }}